---
import "@/styles/global.css";

interface Props {
  title: string;
  description: string;
  image?: string;
}

const { title, description, image = "/placeholder.png" } = Astro.props;
---

<meta charset="UTF-8" />
<meta name="viewport" content="width=device-width" />
<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
<meta name="generator" content={Astro.generator} />

<!-- Google tag (gtag.js) -->
<script async src="https://www.googletagmanager.com/gtag/js?id=G-VFF4VBWT1C"></script>
<script>
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments);}
  gtag('js', new Date());

  gtag('config', 'G-VFF4VBWT1C');
</script>

<!-- Primary Meta Tags -->
<title>PLANET TAKE OVER | Join The Resistance. Master AI Before It Masters You.</title>
<meta name="title" content="PLANET TAKE OVER | Join The Resistance. Master AI Before It Masters You." />
<meta name="description" content="The AI revolution is here. Learn the tools, build systems, and join The Resistance to stay ahead of the takeover." />

<!-- google fonts -->
<link rel="preconnect" href="https://fonts.googleapis.com" />
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
<link
  href="https://fonts.googleapis.com/css2?family=Geist:wght@100..900&family=IBM+Plex+Mono:wght@700&family=Bricolage+Grotesque:opsz,wght@12..24,200..800&display=swap"
  rel="stylesheet"
/>
